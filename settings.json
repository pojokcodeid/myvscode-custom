{
  "css.enabledLanguages": ["html", "tsx", "jsx", "css"],
  "workbench.iconTheme": "symbols",
  "window.menuBarVisibility": "compact",
  "explorer.compactFolders": false,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.enabled": false,
  "window.commandCenter": false,
  "explorer.autoReveal": true,
  // "editor.lineNumbers": "relative",
  "window.zoomLevel": 1,
  "editor.fontSize": 17,
  "editor.lineHeight": 1.7,
  "scm.inputFontSize": 17,
  "debug.console.fontSize": 17,
  "markdown.preview.fontSize": 17,
  "terminal.integrated.fontSize": 16,
  "editor.fontFamily": "'Hasklug Nerd Font','Maple Mono Normal','Iosevka Nerd Font','Source Code Pro',Consolas,'Source Code Pro','Fantasque Sans Mono'",
  "editor.fontLigatures": true,
  "workbench.editor.enablePreview": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "expand",
  "database-client.autoSync": true,
  "workbench.tree.indent": 20,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#52545a00"
  },
  /** ------------ Additional Configurations ------------ **/
  "vscode_custom_css.imports": [
    "file:///D:/custom_vscode/style.css",
    "file:///D:/custom_vscode/script.js"
  ],
  "extensions.ignoreRecommendations": true,
  "editor.tabSize": 4 /* Sets indentation size to 2 spaces globally */,
  "editor.insertSpaces": true /* Formats code automatically on save globally */,
  "editor.formatOnType": true /* Formats code automatically while typing globally */,
  "editor.rulers": [80] /* Draws a guideline at 80 characters */,
  "editor.guides.indentation": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.startupEditor": "none",
  "terminal.integrated.allowChords": false,
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": "off"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr",
    "cfabort",
    "cfapplication",
    "cfargument",
    "cfassociate",
    "cfbreak",
    "cfchartdata",
    "cfcollection",
    "cfcontent",
    "cfcontinue",
    "cfcookie",
    "cfdirectory",
    "cfdump",
    "cfelse",
    "cfelseif",
    "cferror",
    "cfexecute",
    "cfexit",
    "cffile",
    "cfflush",
    "cfheader",
    "cfhttpparam",
    "cfimage",
    "cfimport",
    "cfinclude",
    "cfindex",
    "cfinput",
    "cfinvokeargument",
    "cflocation",
    "cflog",
    "cfloginuser",
    "cflogout",
    "cfmailparam",
    "cfobject",
    "cfobjectcache",
    "cfparam",
    "cfpop",
    "cfprocessingdirective",
    "cfprocparam",
    "cfprocresult",
    "cfproperty",
    "cfqueryparam",
    "cfregistry",
    "cfreportparam",
    "cfrethrow",
    "cfreturn",
    "cfschedule",
    "cfspreadsheet",
    "cfsearch",
    "cfset",
    "cfsetting",
    "cfthrow",
    "cfwddx"
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "cfml": "html",
    "javascript": "javascriptreact"
  },
  "files.associations": {
    "*.sfc": "cfml"
  },
  "code-runner.runInTerminal": true,
  "javascript.suggest.autoImports": true,
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codeium.enableConfig": {
    "*": true,
    "cfml": true,
    "go.mod": true
  },
  "workbench.editor.empty.hint": "hidden",
  "go.toolsManagement.autoUpdate": true,
  "workbench.productIconTheme": "fluenticon",
  "workbench.layoutControl.enabled": false,
  "editor.minimap.enabled": false,
  "workbench.colorTheme": "One Dark Pro Night Flat",
  "hediet.vscode-drawio.resizeImages": null,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // start config whichkey without neovim
  // triger on keybinding
  "whichkey.delay": 0,
  "whichkey.sortOrder": "alphabetically",
  "whichkey.bindings": [
    {
      "key": ";",
      "name": "commands",
      "type": "command",
      "command": "workbench.action.showCommands"
    },
    {
      "key": "c",
      "name": "Chat with AI",
      "type": "command",
      "command": "codeium.openChatView"
    },
    {
      "key": "k",
      "name": "Close active editor",
      "type": "command",
      "command": "workbench.action.closeActiveEditor"
    },
    {
      "key": "q",
      "name": "Kill Terminal",
      "type": "command",
      "command": "workbench.action.terminal.kill"
    },
    {
      "key": "t",
      "name": "Open Terminal",
      "type": "command",
      "command": "workbench.action.terminal.focus"
    },
    {
      "key": "T",
      "name": "Open Float Terminal",
      "type": "command",
      "command": "multiCommand.folatTerminal"
    },
    {
      "key": "b",
      "name": "+ Buffers/Editors...",
      "type": "bindings",
      "bindings": [
        {
          "key": "b",
          "name": "Show all buffers/editors",
          "type": "command",
          "command": "workbench.action.showAllEditors"
        },
        {
          "key": "m",
          "name": "Close other editors",
          "type": "command",
          "command": "workbench.action.closeOtherEditors"
        },
        {
          "key": "u",
          "name": "Reopen closed editor",
          "type": "command",
          "command": "workbench.action.reopenClosedEditor"
        },
        {
          "key": "y",
          "name": "Copy buffer to clipboard",
          "type": "commands",
          "commands": [
            "editor.action.selectAll",
            "editor.action.clipboardCopyAction",
            "cancelSelection"
          ]
        }
      ]
    },
    {
      "key": "g",
      "name": "Toggle Git",
      "type": "command",
      "command": "workbench.view.scm"
    },
    {
      "key": "d",
      "name": "Toggle Debug",
      "type": "command",
      "command": "workbench.view.debug"
    },
    {
      "key": "x",
      "name": "Toggle Extensions",
      "type": "command",
      "command": "workbench.view.extensions"
    },
    {
      "key": "e",
      "name": "Open Exploler",
      "type": "command",
      "command": "workbench.view.explorer",
      "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
      "key": "w",
      "name": "Toggle Sidebar",
      "type": "command",
      "command": "workbench.action.toggleSidebarVisibility",
      "when": "viewContainer.workbench.view.sidebar.enabled"
    },
    {
      "key": "a",
      "name": "ActivityBar Toggle",
      "type": "command",
      "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
      "key": "f",
      "name": "+ Find & Replace...",
      "type": "bindings",
      "bindings": [
        {
          "key": "f",
          "name": "File",
          "type": "command",
          "command": "actions.find",
          "when": "editorFocus || editorIsOpen"
        },
        {
          "key": "F",
          "name": "File & Replace",
          "type": "command",
          "command": "editor.action.startFindReplaceAction"
        },
        {
          "key": "s",
          "name": "Symbol",
          "type": "command",
          "command": "editor.action.rename",
          "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
        },
        {
          "key": "p",
          "name": "Project",
          "type": "command",
          "command": "workbench.action.replaceInFiles"
        }
      ]
    },
    {
      "key": "\\",
      "name": "+ Split",
      "type": "bindings",
      "bindings": [
        {
          "key": "h",
          "name": "Split Horizontal",
          "type": "command",
          "command": "workbench.action.splitEditorDown"
        },
        {
          "key": "v",
          "name": "Split Vertical",
          "type": "command",
          "command": "workbench.action.splitEditor"
        }
      ]
    },
    {
      "key": "s",
      "name": "+ Search...",
      "type": "bindings",
      "bindings": [
        {
          "key": "f",
          "name": "files",
          "type": "command",
          "command": "workbench.action.quickOpen"
        },
        {
          "key": "t",
          "name": "text",
          "type": "command",
          "command": "workbench.action.findInFiles"
        }
      ]
    },
    {
      "key": "z",
      "name": "Toggle zen mode",
      "type": "command",
      "command": "workbench.action.toggleZenMode"
    },
    {
      "key": "u",
      "name": "+ UI",
      "type": "bindings",
      "bindings": [
        {
          "key": "i",
          "name": "Icon Theme",
          "type": "command",
          "command": "workbench.action.selectIconTheme"
        },
        {
          "key": "t",
          "name": "Color Theme",
          "type": "command",
          "command": "workbench.action.selectTheme"
        }
      ]
    },
    {
      "key": "l",
      "name": "+ Live Server",
      "type": "bindings",
      "bindings": [
        {
          "key": "o",
          "name": "Open LiveServer",
          "type": "command",
          "command": "extension.liveServer.goOnline",
          "when": "editorTextFocus"
        },
        {
          "key": "c",
          "name": "Close LiveServer",
          "type": "command",
          "command": "extension.liveServer.goOffline",
          "when": "editorTextFocus"
        }
      ]
    }
  ],
  // note install multi-command
  "multiCommand.commands": {
    "multiCommand.folatTerminal": {
      "sequence": [
        "workbench.action.terminal.new",
        "workbench.action.terminal.moveIntoNewWindow",
        "workbench.action.enableCompactAuxiliaryWindow",
        "workbench.action.enableWindowAlwaysOnTop"
      ]
    }
  },
  "material-icon-theme.hidesExplorerArrows": true,
  "documentViewer.readOnly": false,
  "editor.formatOnSave": true,
  "workbench.activityBar.location": "top"
}
